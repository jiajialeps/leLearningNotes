    var material;

    // depend on sync logic
    require(['SjUp81t/lego/pc/materials/d20/ImageSliderAd/main'], function (Material) {

        function createMaterial() {
            material = new Material({
                data: AD_CONFIG,
                layout: null
            });

            return material;
        }

        // async
        if (typeof ECMA_define === 'function') {
            function factory() {
                return {
                    Material: Material,
                    runtime: RT_CONFIG,
                    data: AD_CONFIG,
                    __source__: 'lego',
                    layout: null
                };
            }
            if (typeof ECMA_wrapper !== 'undefined') {
                // v8
                return ECMA_define(factory);
            }
            if (typeof ECMA_define.getCurrentScript === 'function') {
                var script = ECMA_define.getCurrentScript();
                if (script && script.attributes['data-ecma-module']) {
                    return ECMA_define(factory);
                }
            }
        }

        createMaterial();

        // prerender/preview
        if (typeof legoBootstrap === 'function') {
            legoBootstrap(material);
            return;
        }

        // render (material should be ready)
        material.render();
    });
})(
